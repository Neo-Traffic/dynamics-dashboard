{
  "name": "dynamics-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@svgr/webpack": "5.4.0",
    "axios": "^0.19.2",
    "babel-core": "^7.0.0-beta.41",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-plugin-react-intl": "^7.5.28",
    "babel-preset-react-app": "^9.1.2",
    "bfj": "^7.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^4.1.0",
    "copy-to-clipboard": "^3.3.1",
    "css-loader": "^3.6.0",
    "date-fns": "2",
    "dotenv": "8",
    "dotenv-expand": "5",
    "eslint": "^7.4.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "eslint-plugin-standard": "*",
    "file-loader": "6.0.0",
    "fs-extra": "9.0.1",
    "html-webpack-plugin": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-cookie": "^4.0.3",
    "react-datepicker": "^3.1.3",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-intl": "^5.0.2",
    "react-intl-cra": "^0.3.4",
    "react-intl-po": "^2.2.2",
    "react-intl-translations-manager": "^5.0.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "resolve": "1.17.0",
    "sass-loader": "9.0.2",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "4.1.0",
    "uuid": "^8.2.0",
    "validator": "^13.1.1",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "extract-messages": "NODE_ENV=production babel ./src  --out-file /dev/null && rip json2pot './messages/src/**/*.json' -o ./translation/en-ca.pot",
    "compile-translations": "rip po2json './translation/*.po' -m './messages/src/**/*.json' -o './src/assets/locales'",
    "extract:messages": "react-intl-cra 'src/**/*.{js,jsx}' -o './messages/messages.json' && node ./translationRunner.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "react-intl",
        {
          "messagesDir": "./messages/"
        }
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
